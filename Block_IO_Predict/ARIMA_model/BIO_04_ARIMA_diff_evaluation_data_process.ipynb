    "# 만든이 : 정성모\n",
    "# 입력 : Original_data, Forecast_data\n",
    "# 출력 : 예측 값 total mae\n",
    "# 예측 데이터와 실제 데이터 차이 확인\n",
    "\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import statsmodels.api as sm\n",
    "from numpy import nan\n",
    "from statsmodels.tsa.stattools import adfuller\n",
    "from statsmodels.tsa.arima_model import ARIMA\n",
    "%matplotlib inline\n",
    "import time\n",
    "\n",
    "def main():\n",
    "    data = pd.read_csv(\"C:/Users/jsm97/Desktop/raw193.csv\")\n",
    "    data = data[data['block_getrq']!=0]\n",
    "    data = data.sort_values(['block_getrq'],ascending=\"False\")\n",
    "    data = data.reset_index(drop=True)\n",
    "    data = data[0:50000]\n",
    "    \n",
    "    nvme_sq = pd.read_csv(\"C:/Users/jsm97/Desktop/nv_get.csv\")\n",
    "    nvme_sq = nvme_sq['0']\n",
    "    nvme_sq = nvme_sq[0:50000]\n",
    "    \n",
    "    forecast = pd.read_csv(\"C:/Users/jsm97/Desktop/ARIMA_diff_evaluation_50.csv\")\n",
    "    forecast = forecast['1']\n",
    "    #print(forecast)\n",
    "    \n",
    "    zero_data = nvme_sq[nvme_sq == 0]\n",
    "    \n",
    "    #print(nvme_sq[nvme_sq == 0])\n",
    "    nvme_sq[nvme_sq == 0] = forecast.values\n",
    "    #print(nvme_sq[nvme_sq == 0])\n",
    "    \n",
    "    data['forecast'] = nvme_sq\n",
    "    #data['original'] = data['nvme_sq']\n",
    "    data['original'] = data['nvme_sq']-data['block_getrq']\n",
    "    temp = data[['original','forecast']]\n",
    "    #print(temp)\n",,
    "    \n",
    "    #temp.to_csv(\"C:/Users/jsm97/Desktop/ARIMA_diff_evaluation_10_50000data_test.csv\", index=False)\n",
    "    \n",
    "    # nvme_sq - getrq와 forecast를 원데이터로 바꾸고 mae\n",
    "    zero_data1 = zero_data.reset_index()\n",
    "    \n",
    "    temp = zero_data1['index'].values\n",
    "    orgi = zero_data\n",
    "    for i in temp:\n",
    "        orgi[i] = data.iloc[i]['nvme_sq']\n",
    "    print(orgi)\n",
    "    fore = zero_data\n",
    "    print('----------------------------')\n",
    "    for i in temp:\n",
    "        fore[i] = data.iloc[i]['block_getrq']\n",
    "    fore=fore +forecast.values\n",
    "    print(fore)\n",
    "    \n",
    "    orgi=np.array(orgi).reshape(-1,1)\n",
    "    fore=np.array(fore).reshape(-1,1)\n",
    "    #print(orgi - fore)\n",
    "    error_mean = np.mean(np.abs(orgi - fore), axis=0)\n",
    "    print(\"mae : \",error_mean)\n",
    "    '''\n",
    "    # nvme_sq - getrq와 forecast의 mae\n",
    "    prednvme = data['block_getrq'] + temp['forecast']\n",
    "    print(\"forecast nvme_sq\")\n",
    "    print(prednvme)\n",
    "    prednvme=np.array(prednvme).reshape(-1,1)\n",
    "    \n",
    "    nvme = data['nvme_sq']\n",
    "    print(\"original nvme_sq\")\n",
    "    print(nvme)\n",
    "    nvme=np.array(nvme).reshape(-1,1)\n",
